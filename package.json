{
  "name": "notepack",
  "version": "1.0.0",
  "description": "Local note taking Node helpers",
  "main": "index.js",
  "scripts": {
    "configure": "node src/configure.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "todos": "node src/todos.js",
    "todos:mine": "npm run todos -- --mine",
